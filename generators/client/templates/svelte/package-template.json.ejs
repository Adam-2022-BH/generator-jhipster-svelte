{
	"name": "<%= dasherizedBaseName %>",
	"description": "Description for <%= baseName  %>",
	"version": "0.1.0",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"format": "prettier --write \"{,src/**/}*.{md,json,js,svelte,css,html,yml}\"",
		"lint": "eslint .",
		"lint:fix": "npm run lint --fix",
		"cleanup": "rimraf <%= DIST_DIR %>",
		"start": "svelte-kit dev --port 9000 --open true",
		"preview": "svelte-kit preview",
		"test": "jest <%= CLIENT_MAIN_SRC_DIR %>",
		"test:watch": "npm run test -- --watch",
		"e2e": "npm run e2e:ci -- --headed",
		"e2e:ci": "cypress run --browser chrome",
		"cy:open": "cypress open",
		"build": "svelte-kit build",
		"webapp:build": "npm run build",
		"webapp:prod": "npm run build",
		"webapp:test": "npm run test"
	},
	"devDependencies": {
		"generator-jhipster-svelte": "<%= blueprintjs.version %>"
	},
	"type": "module"
}
