{
	"name": "<%= dasherizedBaseName %>",
	"description": "Description for <%= baseName  %>",
	"version": "0.0.0",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prettier:format": "prettier --write \"{,src/**/}*.{md,json,js,svelte,css}\"",
		"lint": "eslint <%= CLIENT_MAIN_SRC_DIR %>",
		"lint:fix": "npm run lint --fix",
		"cleanup": "rimraf <%= DIST_DIR %>",
		"start": "run-s \"tailwind:build\" \"sapper:watch\"",
		"sapper:watch": "sapper dev --src <%= CLIENT_MAIN_SRC_DIR %>app --routes <%= CLIENT_MAIN_SRC_DIR %>app/routes --static <%= CLIENT_MAIN_SRC_DIR %>static --output <%= CLIENT_MAIN_SRC_DIR %>app/node_modules/@sapper --build-dir <%= BUILD_DIR %>webapp/output --port 9000 --open true",
		"sapper:build": "sapper export <%= DIST_DIR %>  --basepath / --src <%= CLIENT_MAIN_SRC_DIR %>app --routes <%= CLIENT_MAIN_SRC_DIR %>app/routes --static <%= CLIENT_MAIN_SRC_DIR %>static --output <%= CLIENT_MAIN_SRC_DIR %>app/node_modules/@sapper --build-dir <%= BUILD_DIR %>webapp/output",
		"tailwind:build": "postcss <%= CLIENT_MAIN_SRC_DIR %>app/tailwind.css -o <%= CLIENT_MAIN_SRC_DIR %>static/vendor.css",
		"tailwind:watch": "npm run tailwind:build -- --w",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run",
		"webpack:build": "NODE_ENV=development run-s \"tailwind:build\" \"sapper:build\"",
		"webpack:prod": "NODE_ENV=production run-s \"tailwind:build\" \"sapper:build\"",
		"webpack:test": "npm run lint"
	},
	"dependencies": {
		"compression": "1.7.4",
		"polka": "0.5.2",
		"sirv": "1.0.6"
	},
	"devDependencies": {
		"@babel/core": "7.11.4",
		"@babel/plugin-syntax-dynamic-import": "7.8.3",
		"@babel/plugin-transform-runtime": "7.11.0",
		"@babel/preset-env": "7.11.0",
		"@babel/runtime": "7.11.2",
		"@fortawesome/free-solid-svg-icons": "5.14.0",
		"@fullhuman/postcss-purgecss": "2.3.0",
		"@rollup/plugin-babel": "5.2.0",
		"@rollup/plugin-commonjs": "15.0.0",
		"@rollup/plugin-node-resolve": "9.0.0",
		"@rollup/plugin-replace": "2.3.3",
		"autoprefixer": "9.8.6",
		"cssnano": "4.1.10",
		"cypress": "5.0.0",
		"eslint": "7.7.0",
		"eslint-plugin-svelte3": "2.7.3",
		"fa-svelte": "3.1.0",
		"generator-jhipster-svelte": "<%= blueprintjs.version %>",
		"http-proxy-middleware": "1.0.5",
		"husky": "4.2.5",
		"npm-run-all": "4.1.5",
		"postcss-cli": "7.1.2",
		"prettier": "2.1.1",
		"prettier-plugin-svelte": "1.2.0",
		"pretty-quick": "3.0.0",
		"rollup": "2.26.8",
		"rollup-plugin-svelte": "6.0.0",
		"rollup-plugin-terser": "7.0.1",
		"sapper": "0.28.3",
		"svelte": "3.24.1",
		"tailwindcss": "1.7.6"
	}
}
